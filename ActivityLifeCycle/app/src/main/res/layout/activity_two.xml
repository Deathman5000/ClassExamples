<?xml version="1.0" encoding="utf-8"?>

<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.ra412063.activitylifecycle.ActivityTwo">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/createString"
        android:layout_marginLeft="10dp"
        android:textStyle="bold"
        android:text="onCreate(): "/>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/startString"
        android:layout_marginLeft="10dp"
        android:textStyle="bold"
        android:text="onStart(): "/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/resumeString"
        android:layout_marginLeft="10dp"
        android:textStyle="bold"
        android:text="onResume(): "/>


</LinearLayout>

